[configuration]
entry_symbol = "gdext_rust_init"
compatibility_minimum = 4.1
reloadable = true

[libraries]
linux.debug.x86_64 =     "res://../target/debug/gcore.so"
linux.release.x86_64 =   "res://../target/release/gcore.so"
windows.debug.x86_64 =   "res://../target/debug/gcore.dll"
windows.release.x86_64 = "res://../target/release/gcore.dll"
macos.debug =            "res://../target/debug/gcore.dylib"
macos.release =          "res://../target/release/gcore.dylib"
macos.debug.arm64 =      "res://../target/debug/gcore.dylib"
macos.release.arm64 =    "res://../target/release/gcore.dylib"
